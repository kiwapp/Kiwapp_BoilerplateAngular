  "projectsDependencies": [
	    {
	        "dest": "app-setup",
	        "project" : "../../app-setup/build/",
	        "projectGulp" : "../../app-setup/GulpFile.js",
	        "task": "prod"
	    },
	    {
	        "dest": "app-databrowser",
	        "project" : "../../app-databrowser/build/",
	        "projectGulp" : "../../app-databrowser/GulpFile.js",
	        "task": "prod"
	    }
	],
  "defaultPort": 8080
}