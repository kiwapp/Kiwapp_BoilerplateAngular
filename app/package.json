{
  "name": "app",
  "description": "app template",
  "version": "0.0.1",
  "dependencies": {
    "gulp": "*"
  },
  "scripts": {
    "prod": "gulp --gulpfile ~/Projets/Kiwapp_GulpFile/GulpFile.js prod",
  	"serve": "gulp --gulpfile ~/Projets/Kiwapp_GulpFile/GulpFile.js serve",
  	"test":"echo hey"
  },
  "urlApi": "https://api.kiwapp.com/nosecure/appInstance/",
